aws s3api create-bucket --bucket reyaz-kops-testbkt12345.k8s.local --region ap-south-1 --create-bucket-configuration LocationConstraint=ap-south-1

aws s3api put-bucket-versioning --bucket reyaz-kops-testbkt12345.k8s.local --region ap-south-1 --versioning-configuration Status=Enabled

export KOPS_STATE_STORE=s3://reyaz-kops-testbkt12345.k8s.local 

kops create cluster --name reyaz.k8s.local --zones ap-south-1a --control-plane-size t3.small --node-count=2 --node-size t3.micro

kops update cluster --name reyaz.k8s.local --yes --admin
